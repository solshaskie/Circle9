{
	"name": "circle9",
	"description": "Circle9, a Windows-Linux file bridge application",
	"version": "0.4.0",
	"author": "Circle9 Development Team",
	"icon": "build/icon.icns",
	"private": true,
	"homepage": "https://circle9.app",
	"os": [
		"win32"
	],
	"scripts": {
		"start": "yarn dev",
		"dev": "node scripts/dev.js",
		"dev:old": "yarn compile && concurrently --kill-others \"yarn compile:watch\" \"yarn sass:watch\" \"tauri dev\"",
		"clean": "rimraf out",
		"sass": "sass src/Public/style.scss out/src/Public/style.css",
		"sass:watch": "sass --watch src/Public/style.scss:out/src/Public/style.css",
		"pretest": "yarn compile",
		"test": "jest",
		"copyfiles": "node scripts/copyfiles",
		"compile": "tsc && yarn sass && yarn copyfiles",
		"compile:watch": "tsc --watch",
		"lint": "eslint -c .eslintrc.yml --ext .ts ./src",
		"prettier": "prettier --write src",
		"grunt": "grunt",
		"css:minify": "cleancss --batch --batch-suffix \"\" out/**/*.css ",
		"prebuild": "yarn compile && yarn grunt && yarn css:minify",
		"build": "node scripts/build.js",
		"build:tauri": "tauri build",
		"postinstall": "husky install"
	},
	"keywords": [
		"Circle9",
		"File bridge",
		"Windows",
		"Linux",
		"SSH",
		"SFTP",
		"File manager"
	],
	"license": "Apache-2.0",
	"devDependencies": {
		"@tauri-apps/cli": "^1.1.1",
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"buffer": "^6.0.3",
		"clean-css-cli": "^5.3.3",
		"concurrently": "^6.2.1",
		"cpy": "^8.1.2",
		"eslint": "^8.2.0",
		"grunt": "^1.4.1",
		"grunt-cli": "^1.4.3",
		"grunt-contrib-uglify": "^5.0.1",
		"grunt-contrib-watch": "^1.1.0",
		"husky": "^7.0.2",
		"jest": "^27.1.0",
		"node-watch": "^0.7.1",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "2.5.1",
		"rimraf": "^3.0.2",
		"sass": "1.45.2",
		"ts-jest": "^27.0.7",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.2",
		"webpack": "^5.58.2",
		"webpack-cli": "^4.9.0"
	},
	"dependencies": {
		"@tauri-apps/api": "^1.1.0",
		"highlight.js": "^11.2.0",
		"mammoth": "^1.4.18",
		"marked": "^4.0.15",
		"xlsx": "^0.17.1"
	},
	"optionalDependencies": {}
}
